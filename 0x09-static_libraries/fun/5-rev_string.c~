#include "main.h"
/**
 *  rev_string - check the code
 * @s: the character
 * Return: Always 0.
 */
void rev_string(char *s)
{
int r, len;
char strt, end = s;
for (r = 0; s[r] != '\0' && s[r] != '\0'; r++) 
{
end++;
}
len = r + 1;
strt = s;
for (r = 0; r < len / 2; r++)
{
char rev;
rev = *end;
*end = *strt;
*strt = rev;
strt++;
end--;
}
end[len + 1] = '\0'
}
